{
  "name": "vue-music-player",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.12.4",
  "exports": {
    ".": {
      "types": "./dist/types/exports.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./index.css": "./dist/index.css",
    "./tailwind.css": "./dist/tailwind.css",
    "./tailwind.safelist": "./dist/tailwind.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/exports.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:npm": "vite build --mode npm && npm run build:tailwind",
    "build:tailwind": "vite build --config vite.tailwind.config.ts",
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/carbon": "^1.2.10",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^18.19.115",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "bumpp": "^10.2.0",
    "eslint": "^9.30.1",
    "jsdom": "^19.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-class-extractor": "^0.0.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-pages": "^0.32.5",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "stackblitz": {
    "startCommand": "pnpm --filter='./playground' run dev"
  }
}
