{
  "name": "vue-music-vue-tailwind4",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.9.0",
    "exports": {
    ".": {
      "types": "./dist/types/MusicPlayer.vue.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./index.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/exports.d.ts",
  "scripts": {
    "build:npm": "vite build --mode npm",
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify/tailwind": "^1.2.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@vueuse/core": "^9.13.0",
    "postcss": "^8.5.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^18.19.86",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "jsdom": "^19.0.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-class-extractor": "^0.0.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-pages": "^0.32.5",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "stackblitz": {
    "startCommand": "pnpm --filter='./playground' run dev"
  }
